/* pages/profile/profile.wxss */
.avatar{
  width: 100%;
  height: 400rpx;
  background: #AB956D;
  display: flex;
  justify-content: center;
  align-items: center;
}
.avatar image{
  width: 200rpx;
  height: 200rpx;
  border: 10rpx solid rgba(0, 0, 0, 0.3);
  border-radius: 50%;
}
/*列表*/
.cell{
  background-color: #fff;
  font-size: 16px;
}
.cell-item{
  border-bottom: 1rpx solid #ddd;
  padding: 20rpx 30rpx;
}